{
  "name": "@stardust-platform/web-login",
  "version": "1.0.17",
  "description": "Stardust auth library in Typescript!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stardust-Platform/web-login.git"
  },
  "bugs": {
    "url": "https://github.com/Stardust-Platform/web-login/issues",
    "email": "support@stardust.gg"
  },
  "author": "STARDUST CARDS CORPORATION",
  "contributors": [
    "Opticpwr (https://opticpwr.com)"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "watch \"yarn build\" ./src",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build && husky install",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.5.4",
    "@aws-amplify/ui-components": "^1.9.6",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "amazon-cognito-identity-js": "^5.2.8",
    "aws-amplify": "^4.3.22",
    "axios": "^0.27.2",
    "react-google-fonts-css2": "^1.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "27.5.1",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "prettier": "^2.6.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "watch": "^1.0.2",
    "webpack": "5.72.1"
  },
  "files": [
    "/lib"
  ],
  "size-limit": [
    {
      "path": "lib/cjs/index.js",
      "limit": "400 KB"
    },
    {
      "path": "lib/esm/index.js",
      "limit": "400 KB"
    }
  ],
  "prettier": {
    "singleQuote": true
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/Stardust-Platform/web-login#readme",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "keywords": [
    "SSO"
  ]
}