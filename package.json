{
  "name": "@stardust-platform/web-login",
  "version": "1.0.9",
  "description": "Stardust auth library in Typescript!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stardust-Platform/web-login.git"
  },
  "bugs": {
    "url": "https://github.com/Stardust-Platform/web-login/issues",
    "email": "support@stardust.gg"
  },
  "author": "STARDUST CARDS CORPORATION",
  "contributors": [
    "Opticpwr (https://opticpwr.com)"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "watch \"yarn build\" ./src",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build && husky install",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.3.18",
    "@aws-amplify/ui-components": "^1.9.6",
    "amazon-cognito-identity-js": "^5.2.3",
    "aws-amplify": "^4.3.10",
    "axios": "^0.24.0",
    "react-google-fonts-css2": "^1.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "27.4.4",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "graphql": "^16.1.0",
    "husky": "^7.0.4",
    "jest": "27.4.4",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.4",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "watch": "^1.0.2",
    "webpack": "5.65.0"
  },
  "files": [
    "/lib"
  ],
  "size-limit": [
    {
      "path": "lib/cjs/index.js",
      "limit": "400 KB"
    },
    {
      "path": "lib/esm/index.js",
      "limit": "400 KB"
    }
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/Stardust-Platform/web-login#readme",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "keywords": [
    "SSO"
  ]
}
