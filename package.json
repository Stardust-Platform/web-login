{
  "name": "stardust-auth",
  "version": "0.2.1",
  "description": "Stardust auth library in Typescript!",
  "author": "Opticpwr<https://opticpwr.com>",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "unlicense",
  "private": true,
  "scripts": {
    "start": "watch \"yarn build\" ./src",
    "build": "yarn build:esm && yarn build:cjs && yarn run copy-files",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "copy-files": "copyfiles -u 1 \"src/**/*.ttf\" \"lib/cjs/\" && copyfiles -u 1 \"src/**/*.ttf\" \"lib/esm/\"",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build && husky install",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.3.5",
    "@aws-amplify/ui-components": "^1.7.8",
    "amazon-cognito-identity-js": "^5.1.0",
    "aws-amplify": "^4.2.9",
    "copyfiles": "^2.4.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "26.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "graphql": "^15.6.0",
    "husky": "^7.0.2",
    "jest": "26.6.0",
    "prettier": "^2.4.1",
    "size-limit": "^5.0.4",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "watch": "^1.0.2",
    "webpack": "4.44.2"
  },
  "files": [
    "/lib"
  ],
  "size-limit": [
    {
      "path": "lib/cjs/index.js",
      "limit": "400 KB"
    },
    {
      "path": "lib/esm/index.js",
      "limit": "400 KB"
    }
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  }
}
